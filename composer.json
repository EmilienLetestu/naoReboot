{
    "name": "nao",
    "description": "a symfony based web app for twitchers",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "algolia/algolia-search-bundle": "^2.2",
        "phpunit/phpunit": "^6.3",
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/asset": "^3.3",
        "symfony/browser-kit": "^3.3",
        "symfony/console": "^3.3",
        "symfony/expression-language": "^3.3",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/orm-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "^3.3",
        "symfony/swiftmailer-bundle": "^3.0",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/web-server-bundle": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "symfony/dotenv": "^3.3"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BTQ78J177BVKZH22RBHJ8APW",
            "allow-contrib": false
        }
    }
}

